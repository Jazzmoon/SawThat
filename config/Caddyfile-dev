0.0.0.0:80, [::]:80, localhost:80 {
    header -Server
    encode zstd gzip

    root * /app
    file_server

    reverse_proxy /health http://sawthat-backend-dev:6060
    reverse_proxy /api http://sawthat-backend-dev:6060
    reverse_proxy /docs http://api-docs
}
